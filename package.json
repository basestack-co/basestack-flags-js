{
  "name": "@basestack/flags-js",
  "version": "1.0.8",
  "description": "Feature Flags SDK for JavaScript",
  "repository": "https://github.com/basestack-co/basestack-flags-js",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --config tsdown.config.ts",
    "dev": "tsdown --config tsdown.config.ts --watch",
    "clean": "node_modules && rm -rf dist",
    "update:deps": "bun update -i",
    "lint": "bunx biome lint .",
    "format": "bunx biome format --write .",
    "check": "bunx biome check --write .",
    "test": "bunx vitest run",
    "test:watch": "bunx vitest watch",
    "prepublishOnly": "bun run lint && bun run test && bun run build"
  },
  "author": "Basestack",
  "license": "MIT",
  "packageManager": "bun@1.3.4",
  "engines": {
    "node": ">=18.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "tsdown": "^0.17.2",
    "typescript": "^5.8.3",
    "vitest": "^4.0.15"
  },
  "keywords": [
    "feature-flags",
    "feature-flag",
    "feature-toggle",
    "feature-toggles",
    "feature-management",
    "feature-rollout",
    "feature-gating",
    "js",
    "javascript",
    "javascript-sdk",
    "sdk",
    "client-sdk",
    "frontend",
    "backend",
    "node",
    "nodejs",
    "node-js",
    "esm",
    "remote-config",
    "config-management",
    "a-b-testing",
    "experimentation",
    "flags",
    "feature"
  ]
}
